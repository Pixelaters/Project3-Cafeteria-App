<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/submit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Submit"
        tools:layout_editor_absoluteX="158dp"
        tools:layout_editor_absoluteY="611dp" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="49dp"
        android:layout_height="51dp"
        tools:layout_editor_absoluteX="313dp"
        tools:layout_editor_absoluteY="36dp"
        tools:srcCompat="@tools:sample/avatars" />

    <RatingBar
        android:id="@+id/ratingBar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="286dp" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Enjoying the app?"
        tools:layout_editor_absoluteX="150dp"
        tools:layout_editor_absoluteY="220dp" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Give us your feedback"
        tools:layout_editor_absoluteX="136dp"
        tools:layout_editor_absoluteY="239dp" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tell us what you like about it "
        tools:layout_editor_absoluteX="116dp"
        tools:layout_editor_absoluteY="372dp" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="or where we could improve"
        tools:layout_editor_absoluteX="121dp"
        tools:layout_editor_absoluteY="391dp" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="120dp"
        android:layout_height="95dp"
        app:srcCompat="@drawable/cput_logo"
        tools:layout_editor_absoluteX="154dp"
        tools:layout_editor_absoluteY="114dp" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rate Us"
        tools:layout_editor_absoluteX="168dp"
        tools:layout_editor_absoluteY="38dp" />

    <com.google.android.material.textfield.TextInputLayout
        android:layout_width="373dp"
        android:layout_height="108dp"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="476dp">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/feedback"
            android:layout_width="match_parent"
            android:layout_height="125dp"
            android:hint="FeedBack" />
    </com.google.android.material.textfield.TextInputLayout>

</androidx.constraintlayout.widget.ConstraintLayout>